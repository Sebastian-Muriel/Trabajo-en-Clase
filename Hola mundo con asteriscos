/******************************************************************************
Ecribir HOLA MUNDO con asteriscos

*******************************************************************************/
#include <stdio.h>  //se incluye a la libreria//
void main ()   //no devuelve ningun valor//
{
  printf("*  *\t****\t*   \t****\t\t**   **\t*  *\t*   *\t*** \t****\n"); //con printf se imprimio la frase "HOLA MUNDO"//
  printf("*  *\t*  *\t*   \t*  *\t\t* * * *\t*  *\t**  *\t*  *\t*  *\n");
  printf("****\t*  *\t*   \t****\t\t*  *  *\t*  *\t* * *\t*  *\t*  *\n");
  printf("*  *\t*  *\t*   \t*  *\t\t*     *\t*  *\t*  **\t*  *\t*  *\n");
  printf("*  *\t****\t****\t*  *\t\t*     *\t****\t*   *\t*** \t****\n");
  
}
